# .env.example
# Gecko v0.1.1 配置示例

# ========== LLM 配置 ==========
GECKO_DEFAULT_MODEL=gpt-3.5-turbo
GECKO_DEFAULT_API_KEY=your-api-key-here
# GECKO_DEFAULT_BASE_URL=http://localhost:8000/v1
GECKO_DEFAULT_TEMPERATURE=0.7

# ========== 引擎配置 ==========
GECKO_MAX_TURNS=5
GECKO_MAX_CONTEXT_TOKENS=4000

# ========== 存储配置 ==========
GECKO_DEFAULT_STORAGE_URL=sqlite://./gecko_data.db

# ========== 日志配置 ==========
GECKO_LOG_LEVEL=INFO
GECKO_LOG_FORMAT=text

# ========== 性能配置 ==========
GECKO_ENABLE_CACHE=true
GECKO_TOOL_EXECUTION_TIMEOUT=30.0